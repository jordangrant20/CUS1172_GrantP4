{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "front_end/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/demo/(.*)",
      "dest": "/front_end/$1"
    },
    {
      "src": "/demo/?",
      "dest": "/front_end/index.html"
    },
    {
      "src": "/",
      "dest": "/front_end/index.html"
    }
  ]
}
